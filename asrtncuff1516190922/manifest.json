                    "id": "https://nationalgalleryofart.github.io/accademia-documents-iiif/asrtncuff1516190922/canvas/2/annopage-2",
                    "type": "AnnotationPage",
                    "items": [
                        {
                            "id": "https://nationalgalleryofart.github.io/accademia-documents-iiif/asrtncuff1516190922/canvas/2/annopage-2/anno-1",
                            "type": "Annotation",
